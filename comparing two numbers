#include <stdio.h> //including header file

int main() {
   
   int num1, num2; //Initilitation the variables
   
   
   printf("\nEnter the num1: "); //Asking for first number as input
   scanf("%d", &num1);
   
   printf("\nEnter the num2: "); //Asking for second number as input
   scanf("%d", &num2);
   
   if (num1 > num2) //Checking condition if num1 is greater
   {
       
       printf("num1 is greater than num2\n"); //Printing the numbers if num1 is greater
       
   }
   else if(num1 < num2) //Checking condition if num2 is greater
   {
       
       printf("num2 is greater than num1\n"); //Printing the numbers if num2 is greater
   }
   else{ //Checking condition if both are equal
       
       printf("both are equal\n"); //Printing the numbers if both are equal
   }
    return 0;
}
