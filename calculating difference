#include <stdio.h> //including header file

int main() {
   
int num1, num2, difference=0; //Deceleration and initilition the variables

printf("\nEnter the first number: "); //Asking for first number as input
scanf("%d", &num1);

printf("\nEnter the second number: "); //Asking for second number ad input
scanf("%d", &num2);

difference = num1 - num2; //Calculating difference

printf("\ndifference of %d and %d is: %d", num1, num2, difference); //Printing the difference of numbers

    return 0;
}
